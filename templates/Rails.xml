<?xml version="1.0" encoding="UTF-8"?>
<templateSet group="Rails">
  <template name="end" value="&lt;% end %&gt;" description="&lt;% end %&gt;" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="RHTML" value="true" />
    </context>
  </template>
  <template name="if" value="&lt;% if $CONDITION$ %&gt;&#10;  $CURSOR$&#10;&lt;% end %&gt;" description="&lt;% if ... %&gt; ... &lt;% end %&gt;" toReformat="true" toShortenFQNames="true">
    <variable name="CONDITION" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CURSOR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="RHTML" value="true" />
    </context>
  </template>
  <template name="link_to" value="&lt;%= link_to &quot;$END$&quot; %&gt;" description="&lt;%= link_to %&gt;" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="RHTML" value="true" />
    </context>
  </template>
  <template name="redirect_to" value="redirect_to root_path" description="redirect_to root_path" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="RUBY" value="true" />
    </context>
  </template>
  <template name="unless" value="&lt;% unless $CONDITION$ %&gt;&#10;  $CURSOR$&#10;&lt;% end %&gt;" description="&lt;% unless ... %&gt; ... &lt;% end %&gt;" toReformat="true" toShortenFQNames="true">
    <variable name="CONDITION" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CURSOR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="RHTML" value="true" />
    </context>
  </template>
  <template name="else" value="&lt;% else %&gt;" description="&lt;% else %&gt;" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="RHTML" value="true" />
    </context>
  </template>
  <template name="render" value="&lt;%= render '$PARTIAL$' %&gt;" description="&lt;%= render '...' %&gt;" toReformat="true" toShortenFQNames="true">
    <variable name="PARTIAL" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="RHTML" value="true" />
    </context>
  </template>
  <template name="form_for" value="&lt;%= form_for($MODEL$) do |f| %&gt;&#10;  $CURSOR$&#10;  &lt;%= f.submit &quot;Action&quot;, class: &quot;btn&quot; %&gt;&#10;&lt;% end %&gt;" description="&lt;%= form_for ... do |f| %&gt;...&lt;% end %&gt;" toReformat="true" toShortenFQNames="true">
    <variable name="MODEL" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CURSOR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="RHTML" value="true" />
    </context>
  </template>
  <template name="script" value="&lt;script type=&quot;text/javascript&quot; charset=&quot;utf-8&quot;&gt;&#10;$SELECTION$&#10;&lt;/script&gt;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="SELECTION" expression="" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
  </template>
</templateSet>

